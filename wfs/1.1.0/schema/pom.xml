<project xmlns="http://maven.apache.org/POM/4.0.0" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>de.disy.ogc</groupId>
	<artifactId>wfs-v_1_1_0-schema</artifactId>
	<packaging>jar</packaging>
	<name>OGC WFS Schema 1.1.0</name>
	<parent>
		<groupId>de.disy.ogc</groupId>
		<artifactId>wfs-v_1_1_0</artifactId>
		<version>1.0</version>
	</parent>
	<dependencies>
		<dependency>
			<groupId>com.sun.xml.bind</groupId>
			<artifactId>jaxb-impl</artifactId>
		</dependency>
		<dependency>
			<groupId>org.jvnet.jaxbcommons</groupId>
			<artifactId>jaxbcommons-shared</artifactId>
		</dependency>
		<dependency>
			<groupId>org.jvnet.jaxbcommons</groupId>
			<artifactId>jaxbcommons-runtime</artifactId>
		</dependency>
	</dependencies>
	<build>
		<defaultGoal>install</defaultGoal>
		<plugins>
			<plugin>
				<groupId>org.jvnet.jaxb2.maven2</groupId>
				<artifactId>maven-jaxb2-plugin</artifactId>
				<dependencies>
					<dependency>
						<groupId>dom4j</groupId>
						<artifactId>dom4j</artifactId>
						<version>1.6.1</version>
					</dependency>
				</dependencies>
				<configuration>
<!--					<schemaLanguage>xml</schemaLanguage>
-->					<schemaIncludes>
						<value>wfs/*/*.xsd</value>
					</schemaIncludes>
				<source>1.5</source>
				<target>1.5</target>
				</configuration>
			</plugin>
			<plugin>
				<inherited>true</inherited>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<configuration>
				<source>1.5</source>
				<target>1.5</target>
				</configuration>
			</plugin>
		<plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
		</plugins>
	</build>
</project>